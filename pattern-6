#include <bits/stdc++.h>
using namespace std;
int fn(int n){
    int i,j;
    char k='A';
    for(i=0;i<n;i++){
        for(j=0;j<=i;j++){
            cout<<k;
            
        }
        k++;
          
            
        
        cout<<endl;

    }
    return 0;
}


int main() {
    int n;
    cin>>n;
    fn(n);

    return 0;
}
